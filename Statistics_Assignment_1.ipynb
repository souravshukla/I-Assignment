    "\n",
    "func = lambda d: np.exp(-np.power((d - mean),2)/(2*variance))/(std*(2*np.pi)**0.5)\n",
    "\n",
    "print(normal_distribution(func, 13, np.inf))\n",
    "print(normal_distribution(func, 9, 11))"
